<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>


<ScrollPane
    xmlns="http://javafx.com/javafx/23"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="controller.PaperTabController"
    fitToWidth="true">

    <content>
        <VBox spacing="12">

            <TableView fx:id="paperTable">
                <columns>
                    <TableColumn fx:id="qtyCol" text="Qty"/>
                    <TableColumn fx:id="sizeCol" text="Size"/>
                    <TableColumn fx:id="gsmCol" text="GSM"/>
                    <TableColumn fx:id="amountCol" text="Amount"/>
                </columns>
            </TableView>

        </VBox>
    </content>
</ScrollPane>

